<configuration>
  <root level="info">
    <appender-ref ref="CONSOLE" />
  </root>

  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%msg%n</pattern>
    </encoder>
  </appender>

  <logger name="org.thymeleaf" level="ERROR"/>
  <logger name="com.intuit.karate" level="INFO"/>
</configuration>
